{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-tap-highlight-color"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "button-type": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": "off"
  }
}